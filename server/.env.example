# Server Configuration
PORT=8000
HOST=0.0.0.0

# CORS Configuration
ALLOWED_ORIGINS=https://your-vercel-app-url.vercel.app,http://localhost:5173,http://localhost:3000

# Security
ENABLE_RATE_LIMIT=true
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_DURATION=60

# Storage
DOWNLOAD_DIR=downloads
COOKIE_DIR=/tmp
COOKIE_EXPIRY_HOURS=1

# Browser Configuration
YT_DLP_BROWSER=chrome

# Features
ENABLE_DOWNLOAD_CLEANUP=true
AUTO_CLEANUP_DAYS=7
